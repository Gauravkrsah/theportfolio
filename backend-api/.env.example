# Supabase Configuration
VITE_NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
VITE_NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
VITE_SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
VITE_SUPABASE_DB_PASSWORD=your-supabase-db-password

# Direct PostgreSQL connection strings
VITE_DIRECT_CONNECTION=postgresql://postgres:password@your-db-host:5432/postgres
VITE_TRANSACTION_POOLER=postgresql://postgres.id:password@your-pooler-host:6543/postgres
VITE_SESSION_POOLER=postgresql://postgres.id:password@your-pooler-host:5432/postgres

# Google APIs
GEMINI_API_KEY=your-gemini-api-key

# Server Configuration
PORT=4000
NODE_ENV=development
